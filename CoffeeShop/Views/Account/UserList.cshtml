@using CoffeeShop.Identity
@model IEnumerable<AppUser>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<section class="content">
    <div class="row">
        <div class="col-xs-12">


            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">User List</h3>
                </div><!-- /.box-header -->

                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Surname</th>
                                <th>Username</th>
                               
                               
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @item.Name

                                    </td>
                                    <td>
                                        @item.Surname
                                    </td>

                                    <td>
                                        @item.UserName
                                    </td>
                                   

                                </tr>
                            }


                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Name</th>
                                <th>Surname</th>
                                <th>Username</th>
                            </tr>
                        </tfoot>
                    </table>
                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section>

